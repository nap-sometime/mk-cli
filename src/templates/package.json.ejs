{
    "name": "<%= props.appName %>",
		"version": "<%= props.appVersion %>",
		<%_ if (props.appDescription) { _%>
		"description": "<%= props.appDescription %>",
		<%_ } _%>
    "private": true,
		"author": "<%= props.appAuthor %>",
		"scripts": {
			"serve": "vue-cli-service serve",
			"dev": "vue-cli-service serve",
			"build": "vue-cli-service build",
			"lint": "vue-cli-service lint"
		},
		"dependencies": {
			"core-js": "^3.6.5",
			"single-spa-vue": "^1.5.2",
			"systemjs-webpack-interop": "^2.1.1",
			<%_ if (props.vueModules.includes('vue-meta')) { _%>
			"vue-meta": "^2.4.0",
			<%_ } _%>
			<%_ if (props.vueModules.includes('vue-router')) { _%>
			"vue-router": "^3.3.2",
			<%_ } _%>
			"vue": "^2.6.11"
		},
		"devDependencies": {
			"@babel/preset-typescript": "^7.10.1",
			"@types/js-cookie": "^2.2.6",
			"@types/node": "^12",
			"@vue/cli-plugin-babel": "~4.4.0",
			"@vue/cli-plugin-eslint": "~4.4.0",
			"@vue/cli-plugin-typescript": "^4.4.1",
			"@vue/cli-service": "~4.4.0",
			"babel": "^6.23.0",
			"babel-eslint": "^10.1.0",
			"eslint": "^7.2.0",
			"eslint-config-prettier": "^6.11.0",
			"eslint-plugin-prettier": "^3.1.3",
			"eslint-plugin-vue": "^6.2.2",
			"js-cookie": "^2.2.1",
			"prettier": "^2.0.5",
			"typescript": "^3.9.5",
			"vue-cli-plugin-single-spa": "~1.1.2",
			"vue-template-compiler": "^2.6.11"
		}
}